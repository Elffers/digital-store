<h1> This is your cart</h1>
{{ id }}<br>
these are items: {{ items }}
<div>
  <table>
    <thead>
      <th>Product</th>
      <th>Quantity</th>
      <th>Price</th>
    </thead>
    <tbody>
    {{# each items}}
      <tr>
        <td>{{ this.product.name }}</td>
        <td>{{ this.quantity }}</td>
        <td>{{ format-currency this.product.price }}</td>
      </tr>
      {{/ each}}
      <tr>
        <td><strong>Subtotal</strong></td>
        <td></td>
        <td> {{ subtotal }} </td>

      </tr>
    </tbody>
  </table>
</div>
