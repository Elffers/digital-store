<div>
  <h1>{{ customer_name }} <small>{{ email }}</small></h1>
  <ul>
    <li>Total: {{ format-currency total }}</li>
    <li>Status: {{ status }}</li>
    <li>credit card: {{ credit_card }}</li>
    <li>credit card: {{ cvv }}</li>
  </ul>
  <ul>
    {{# each cart.items}}
    <li>{{ this.product.name }}</li>
    {{/each}}
  </ul>
</div>